{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-console": false,
        "no-empty": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "adjacent-overload-signatures": false,
        "max-line-length": [
            false
        ],
        "variable-name": [
            false,
            "check-format"
        ]
    },
    "rulesDirectory": []
}